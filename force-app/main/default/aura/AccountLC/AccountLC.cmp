<aura:component controller="AccountControllerAC" implements="flexipage:availableForRecordHome,force:hasRecordId" access="global" >
    <aura:attribute name="accList" type="List"/>
    <aura:handler name="init" value="{!this}" action="{!c.doinit}" />
    
    <lightning:select name="select1"  label = "Select an account">
        <option value="null">None</option>
        <aura:iteration items ="{!v.accList}"  var = "item" >
        	<option value="{!item.id}">{!item.Name}</option>    
        </aura:iteration>
    </lightning:select>
	
</aura:component>